{
	"name": "mjohnson/packager",
	"type": "library",
	"description": "A dependency and script packager for static assets.",
	"keywords": ["packager", "package", "manifest", "scripts", "dependency", "minifier", "archive", "zip", "css", "js"],
	"homepage": "http://milesj.me/code/php/packager",
	"license": "MIT",
	"authors": [
		{
			"name": "Miles Johnson",
			"homepage": "http://milesj.me"
		}
	],
	"require": {
		"php": ">=5.3"
	},
	"require-dev": {
		"nick4fake/jsmin": "dev-master",
		"natxet/CssMin": "dev-master"
	},
	"suggest": {
		"nick4fake/jsmin": "Provides JsMin for Javascript compression",
		"natxet/CssMin": "Provides CssMin for CSS compression"
	},
	"support": {
		"source": "https://github.com/milesj/Packager"
	},
	"autoload": {
		"psr-0": { "Packager": "src/" }
	}
}
